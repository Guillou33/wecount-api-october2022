{
  "name": "wecount-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "ingest-data:build": "tsc -w",
    "ingest-data:run": "NODE_ENV=dev nodemon build/command/ingestActivityData.js",
    "build": "tsc",
    "init:build": "tsc -w",
    "init:run": "NODE_ENV=dev nodemon build/index.js",
    "start": "NODE_ENV=production node build/index.js",
    "start-staging": "NODE_ENV=staging node build/index.js",
    "dev": "concurrently npm:init:*",
    "start-ingest": "concurrently npm:ingest-data:*",
    "typeorm": "node --require tsconfig-paths/register --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:dev": "NODE_ENV=dev node --require tsconfig-paths/register --require ts-node/register ./node_modules/typeorm/cli.js",
    "test": "jest --watch-all --run-in-band --no-cache",
    "test-ci": "jest --run-in-band --no-cache --ci --reporters=default --reporters=jest-junit --forceExit"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ]
  },
  "_moduleAliases": {
    "@config": "./config",
    "@root": "./build",
    "@event": "./build/event",
    "@service": "./build/service",
    "@controller": "./build/controller",
    "@entity": "./build/entity",
    "@dto": "./build/dto",
    "@repository": "./build/repository",
    "@manager": "./build/manager",
    "@deep/responseError": "./build/service/core/error/response",
    "@deep/routing": "./build/service/core/routing",
    "@deep/database": "./build/service/core/database"
  },
  "repository": "git@gitlab.com:wecount1/wecount-api.git",
  "author": "Thomas Dournet <thomas@weepulse.fr>",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^5.26.0",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.8",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.180",
    "@types/node-mailjet": "^3.3.4",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "concurrently": "^5.3.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "csv-parse": "^4.12.0",
    "csv-stringify": "^5.5.3",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.6.1",
    "import": "^0.0.6",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mixpanel": "^0.13.0",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "newrelic": "^8.16.0",
    "node-mailjet": "^3.3.1",
    "nodemon": "^2.0.5",
    "password-validator": "^5.2.1",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "sib-api-v3-sdk": "^8.0.2",
    "stackify-node-apm": "^1.16.5",
    "typeorm": "^0.2.28",
    "typeorm-naming-strategies": "^2.0.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.11.8",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.0",
    "jest-junit": "^12.2.0",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
